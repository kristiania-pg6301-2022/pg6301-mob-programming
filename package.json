{
  "name": "pg6301-mob-programming",
  "version": "1.0.0",
  "description": "Mock exam for PG6301 Web Development and API design, 2022\r =========================================================",
  "main": "index.js",
  "scripts": {
    "push": "npm run push:github && npm run push:heroku",
    "push:github": "git push github main",
    "push:heroku": "git push heroku main",
    "dev": "concurrently npm:dev:client npm:dev:server",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "verify": "npm run prettier:check",
    "verify:root": "npm run verify:client && npm run verify:server",
    "verify:client": "cd client && npm run test:coverage",
    "verify:server": "cd server && npm run test:coverage",
    "prepare": "husky install",
    "pre-commit": "npm run prettier:check",
    "prettier:check": "prettier --check **/*.{js,jsx}",
    "build": "npm install --include=dev && npm run build:server && npm run build:client",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "start": "cd server && npm start",
    "test": "npm run test:client && npm run test:server",
    "test:client": "cd client && npm run test",
    "test:server": "cd server && npm run test",
    "postinstall": "npm run install:client && npm run install:server",
    "install:client": "cd client && npm install --include=dev",
    "install:server": "cd server && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristiania-pg6301-2022/pg6301-mob-programming.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kristiania-pg6301-2022/pg6301-mob-programming/issues"
  },
  "homepage": "https://github.com/kristiania-pg6301-2022/pg6301-mob-programming#readme",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "husky": "^7.0.4"
  },
  "dependencies": {
    "@parcel/transformer-react-refresh-wrap": "^2.5.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  }
}
